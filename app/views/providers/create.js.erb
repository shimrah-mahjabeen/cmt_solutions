<% provider = @response[:provider] %>

<% if provider.present? %>
  $('#<%= provider.number %>').remove();

  $("<tr id=<%= provider.number %> ><td><%= provider.number %></td><td><%= provider.enumeration_type %></td><td><%= provider.full_name %></td><td><%= provider.created_epoch %></td><td><%= provider.last_updated_epoch %></td><td><%= provider.basic['gender'] %></td><td><%= provider.basic['status'] %></td><td><%= provider.basic['credential'] %></td><td><%= provider.basic['sole_proprietor'] %></td><td><%= provider.basic['enumeration_date'] %></td><td><%= provider.basic['last_updated'] %></td></tr").prependTo("#provider_body");

  $('#provider_number').val('')
<% end %>
